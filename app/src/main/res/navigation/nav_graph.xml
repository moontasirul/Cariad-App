    android:id="@+id/nav_graph"
    app:startDestination="@id/poiFragment">

    <fragment
        android:id="@+id/poiFragment"
        android:name="com.moon.mapApplicationTest.ui.poiList.POIFragment"
        android:label="@string/app_name"
